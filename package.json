{
  "publisher": "vnehruji",
  "name": "verition-worktree-manager",
  "displayName": "Verition Worktree Manager",
  "description": "Effortless multi-workspace management and fast Git project cloning across directories.",
  "version": "1.0.0",
  "packageManager": "pnpm@10.20.0",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other",
    "SCM Providers"
  ],
  "keywords": [
    "git worktree manager",
    "git project manager/copying",
    "multi-workspace",
    "switch branch",
    "switch folder",
    "run command",
    "git 工作树",
    "git 项目管理/复制",
    "多工作区",
    "切换分支",
    "切换文件夹",
    "运行命令",
    "cursor",
    "Trae"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vanessanehruji/verition-worktree-manager.git"
  },
  "bugs": {
    "url": "https://github.com/vanessanehruji/verition-worktree-manager/issues"
  },
  "badges": [
    {
      "url": "https://img.shields.io/badge/PRs-welcome-brightgreen.svg",
      "description": "PRs Welcome",
      "href": "https://github.com/vanessanehruji/verition-worktree-manager/fork"
    }
  ],
  "icon": "images/icon.png",
  "activationEvents": [],
  "main": "./dist/extension.js",
  "l10n": "./l10n",
  "contributes": {
    "commands": [
      {
        "command": "verition-worktree-manager.switchWorktree",
        "title": "%cmd.switchWorktree%",
        "category": "Worktree"
      },
      {
        "command": "verition-worktree-manager.switchToSelectWorkspace",
        "title": "%cmd.switchToSelectWorkspace%",
        "icon": "$(arrow-right)"
      },
      {
        "command": "verition-worktree-manager.refreshWorktree",
        "title": "%cmd.freshWorktree%",
        "icon": "$(refresh)"
      },
      {
        "command": "verition-worktree-manager.refreshGitFolder",
        "title": "%cmd.refreshGitFolder%",
        "icon": "$(refresh)"
      },
      {
        "command": "verition-worktree-manager.refreshRecentFolder",
        "title": "%cmd.refreshFolder%",
        "icon": "$(refresh)"
      },
      {
        "command": "verition-worktree-manager.refreshFavorite",
        "title": "%cmd.refreshFavorite%",
        "icon": "$(refresh)"
      },
      {
        "command": "verition-worktree-manager.addWorktree",
        "title": "%cmd.addWorktree%",
        "icon": "$(new-folder)",
        "category": "Worktree"
      },
      {
        "command": "verition-worktree-manager.addGitFolder",
        "title": "%cmd.addGitFolder%",
        "icon": "$(add)",
        "category": "Worktree"
      },
      {
        "command": "verition-worktree-manager.removeWorktree",
        "title": "%cmd.removeWorktree%",
        "icon": "$(close)"
      },
      {
        "command": "verition-worktree-manager.addWorktreeFromBranch",
        "title": "%cmd.addWorktreeFromBranch%",
        "icon": "$(new-folder)"
      },
      {
        "command": "verition-worktree-manager.revealInSystemExplorer",
        "title": "%cmd.revealInSystemExplorer%",
        "icon": "$(folder-opened)"
      },
      {
        "command": "verition-worktree-manager.revealInSystemExplorer.context",
        "title": "%cmd.revealInSystemExplorer%",
        "icon": "$(folder-opened)"
      },
      {
        "command": "verition-worktree-manager.repairWorktree",
        "title": "%cmd.repairWorktree%"
      },
      {
        "command": "verition-worktree-manager.moveWorktree",
        "title": "%cmd.moveWorktree%"
      },
      {
        "command": "verition-worktree-manager.lockWorktree",
        "title": "%cmd.lockWorktree%",
        "icon": "$(lock)"
      },
      {
        "command": "verition-worktree-manager.unlockWorktree",
        "title": "%cmd.unlockWorktree%",
        "icon": "$(unlock)"
      },
      {
        "command": "verition-worktree-manager.pullWorktree",
        "title": "%cmd.pullWorktree%",
        "icon": "$(repo-pull)"
      },
      {
        "command": "verition-worktree-manager.pushWorktree",
        "title": "%cmd.pushWorktree%",
        "icon": "$(repo-push)"
      },
      {
        "command": "verition-worktree-manager.pruneWorktree",
        "title": "%cmd.pruneWorktree%",
        "icon": "$(wand)"
      },
      {
        "command": "verition-worktree-manager.removeGitFolder",
        "title": "%cmd.removeGitFolder%",
        "icon": "$(close)"
      },
      {
        "command": "verition-worktree-manager.removeMultiGitFolder",
        "title": "%cmd.removeMultiGitFolder%",
        "icon": "$(trash)"
      },
      {
        "command": "verition-worktree-manager.removeFavorite",
        "title": "%cmd.removeFavorite%",
        "icon": "$(close)"
      },
      {
        "command": "verition-worktree-manager.removeMultiFavorite",
        "title": "%cmd.removeMultiFavorite%",
        "icon": "$(trash)"
      },
      {
        "command": "verition-worktree-manager.renameGitFolder",
        "title": "%cmd.renameGitFolder%",
        "icon": "$(edit)"
      },
      {
        "command": "verition-worktree-manager.openTerminal",
        "title": "%cmd.openTerminal%",
        "icon": "$(terminal)"
      },
      {
        "command": "verition-worktree-manager.openExternalTerminal",
        "title": "%cmd.openExternalTerminal%",
        "icon": "$(terminal-bash)"
      },
      {
        "command": "verition-worktree-manager.openExternalTerminal.context",
        "title": "%cmd.openExternalTerminal%",
        "icon": "$(terminal-bash)"
      },
      {
        "command": "verition-worktree-manager.addToWorkspace",
        "title": "%cmd.addToWorkspace%"
      },
      {
        "command": "verition-worktree-manager.removeFromWorkspace",
        "title": "%cmd.removeFromWorkspace%"
      },
      {
        "command": "verition-worktree-manager.copyFilePath",
        "title": "%cmd.copyFilePath%"
      },
      {
        "command": "verition-worktree-manager.openRecent",
        "title": "%cmd.openRecent%",
        "icon": "$(search)"
      },
      {
        "command": "verition-worktree-manager.openFavorite",
        "title": "%cmd.openFavorite%",
        "icon": "$(search)"
      },
      {
        "command": "verition-worktree-manager.openWorkspaceWorktree",
        "title": "%cmd.searchAllWorktree%",
        "icon": "$(search)"
      },
      {
        "command": "verition-worktree-manager.addToGitFolder",
        "title": "%cmd.addToGitFolder%"
      },
      {
        "command": "verition-worktree-manager.addRootsToRepo",
        "title": "%cmd.addToGitFolder%",
        "icon": "$(star-empty)"
      },
      {
        "command": "verition-worktree-manager.addToFavorite",
        "title": "%cmd.addToFavorite%",
        "icon": "$(pin)"
      },
      {
        "command": "verition-worktree-manager.checkoutBranch",
        "title": "%cmd.checkoutBranch%",
        "icon": "$(source-control)",
        "category": "Worktree"
      },
      {
        "command": "verition-worktree-manager.gitFolderViewAsTree",
        "title": "%cmd.viewAsList%",
        "icon": "$(list-tree)",
        "enablement": "gwm.context.gitFolderViewAsTree"
      },
      {
        "command": "verition-worktree-manager.gitFolderViewAsList",
        "title": "%cmd.viewAsTree%",
        "icon": "$(list-flat)",
        "enablement": "!gwm.context.gitFolderViewAsTree"
      },
      {
        "command": "verition-worktree-manager.gitFolderSetOpen",
        "title": "%cmd.gitFolderSetOpen%",
        "icon": "$(root-folder)"
      },
      {
        "command": "verition-worktree-manager.gitFolderSetClose",
        "title": "%cmd.gitFolderSetClose%",
        "icon": "$(root-folder-opened)"
      },
      {
        "command": "verition-worktree-manager.searchAllWorktree",
        "title": "%cmd.searchAllWorktree%",
        "icon": "$(search)",
        "category": "Worktree"
      },
      {
        "command": "verition-worktree-manager.loadAllTreeData",
        "title": "%cmd.loadAllTreeData%",
        "icon": "$(unfold)"
      },
      {
        "command": "verition-worktree-manager.viewHistory",
        "title": "%cmd.viewHistory%",
        "icon": "$(history)"
      },
      {
        "command": "verition-worktree-manager.fetchWorktree",
        "title": "%cmd.fetchWorktree%",
        "icon": "$(repo-fetch)"
      },
      {
        "command": "verition-worktree-manager.fetchRepo",
        "title": "%cmd.fetchRepo%",
        "icon": "$(repo-fetch)"
      },
      {
        "command": "verition-worktree-manager.toggleLog",
        "title": "%cmd.toggleLog%",
        "icon": "$(list-unordered)"
      },
      {
        "command": "verition-worktree-manager.openRepository",
        "title": "%cmd.openRepository%",
        "icon": "$(repo)"
      },
      {
        "command": "verition-worktree-manager.deleteBranch",
        "title": "%cmd.deleteBranch%"
      },
      {
        "command": "verition-worktree-manager.openSetting",
        "title": "%cmd.openSetting%",
        "icon": "$(gear)"
      },
      {
        "command": "verition-worktree-manager.bundleRepo",
        "title": "%cmd.backupRepo%",
        "icon": "$(archive)"
      }
    ],
    "keybindings": [
      {
        "command": "verition-worktree-manager.searchAllWorktree",
        "key": "ctrl+shift+r"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "verition-worktree-manager",
          "icon": "images/icon.svg",
          "title": "%activitybar.verition-worktree-manager%"
        }
      ]
    },
    "views": {
      "verition-worktree-manager": [
        {
          "id": "verition-worktree-manager-list",
          "name": "%view.verition-worktree-manager-list%",
          "visibility": "visible",
          "when": "!config.verition-worktree-manager.treeView.toSCM"
        },
        {
          "id": "verition-worktree-manager-folders",
          "name": "%view.verition-worktree-manager-folders%",
          "visibility": "visible",
          "when": "!config.verition-worktree-manager.treeView.toSCM"
        },
        {
          "id": "verition-worktree-manager-recent",
          "name": "%view.verition-worktree-manager-recent%",
          "visibility": "collapsed"
        },
        {
          "id": "verition-worktree-manager-favorite",
          "name": "%view.verition-worktree-manager-favorite%",
          "visibility": "collapsed"
        },
        {
          "id": "verition-worktree-manager-setting",
          "name": "%view.verition-worktree-manager-setting%",
          "visibility": "visible"
        }
      ],
      "scm": [
        {
          "id": "verition-worktree-manager-list.scm",
          "name": "%view.verition-worktree-manager-list%",
          "visibility": "visible",
          "when": "config.verition-worktree-manager.treeView.toSCM"
        },
        {
          "id": "verition-worktree-manager-folders.scm",
          "name": "%view.verition-worktree-manager-folders%",
          "visibility": "visible",
          "when": "config.verition-worktree-manager.treeView.toSCM"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "verition-worktree-manager.refreshWorktree",
          "when": "view =~ /verition-worktree-manager-list/",
          "group": "navigation@100"
        },
        {
          "command": "verition-worktree-manager.openSetting",
          "when": "view =~ /verition-worktree-manager-list/",
          "group": "navigation@90"
        },
        {
          "command": "verition-worktree-manager.viewHistory",
          "when": "view =~ /verition-worktree-manager-list/",
          "group": "navigation@20"
        },
        {
          "command": "verition-worktree-manager.addRootsToRepo",
          "when": "view =~ /verition-worktree-manager-list/ && gwm.context.addRootsToRepo",
          "group": "navigation@1"
        },
        {
          "command": "verition-worktree-manager.addWorktree",
          "when": "view =~ /verition-worktree-manager-list/",
          "group": "navigation@2"
        },
        {
          "command": "verition-worktree-manager.openWorkspaceWorktree",
          "when": "view =~ /verition-worktree-manager-list/",
          "group": "navigation@3"
        },
        {
          "command": "verition-worktree-manager.pruneWorktree",
          "when": "view =~ /verition-worktree-manager-list/",
          "group": "navigation@5"
        },
        {
          "command": "verition-worktree-manager.toggleLog",
          "when": "view =~ /verition-worktree-manager-list/",
          "group": "navigation@10"
        },
        {
          "command": "verition-worktree-manager.addGitFolder",
          "when": "view =~ /verition-worktree-manager-folders/",
          "group": "navigation@1"
        },
        {
          "command": "verition-worktree-manager.removeMultiGitFolder",
          "when": "view =~ /verition-worktree-manager-folders/",
          "group": "navigation@1"
        },
        {
          "command": "verition-worktree-manager.removeMultiFavorite",
          "when": "view =~ /verition-worktree-manager-favorite/",
          "group": "navigation@1"
        },
        {
          "command": "verition-worktree-manager.gitFolderViewAsTree",
          "when": "view =~ /verition-worktree-manager-folders/ && gwm.context.gitFolderViewAsTree",
          "group": "navigation@2"
        },
        {
          "command": "verition-worktree-manager.gitFolderViewAsList",
          "when": "view =~ /verition-worktree-manager-folders/ && !gwm.context.gitFolderViewAsTree",
          "group": "navigation@2"
        },
        {
          "command": "verition-worktree-manager.searchAllWorktree",
          "when": "view =~ /verition-worktree-manager-folders/",
          "group": "navigation@3"
        },
        {
          "command": "verition-worktree-manager.refreshGitFolder",
          "when": "view =~ /verition-worktree-manager-folders/",
          "group": "navigation@5"
        },
        {
          "command": "verition-worktree-manager.refreshRecentFolder",
          "when": "view === verition-worktree-manager-recent",
          "group": "navigation@2"
        },
        {
          "command": "verition-worktree-manager.openRecent",
          "when": "view === verition-worktree-manager-recent",
          "group": "navigation@1"
        },
        {
          "command": "verition-worktree-manager.refreshFavorite",
          "when": "view === verition-worktree-manager-favorite",
          "group": "navigation@100"
        },
        {
          "command": "verition-worktree-manager.openFavorite",
          "when": "view === verition-worktree-manager-favorite",
          "group": "navigation@90"
        }
      ],
      "view/item/context": [
        {
          "command": "verition-worktree-manager.openTerminal",
          "when": "viewItem =~ /verition-worktree-manager\\.(worktreeItem|gitFolderItem|folderItem)/",
          "group": "terminal@2"
        },
        {
          "command": "verition-worktree-manager.openExternalTerminal.context",
          "when": "viewItem =~ /verition-worktree-manager\\.(worktreeItem|gitFolderItem|folderItem)/",
          "group": "terminal@1"
        },
        {
          "command": "verition-worktree-manager.addWorktreeFromBranch",
          "when": "view =~ /verition-worktree-manager-list/ && viewItem == verition-worktree-manager.branchItem",
          "group": "verition-worktree-action"
        },
        {
          "command": "verition-worktree-manager.revealInSystemExplorer.context",
          "when": "viewItem =~ /verition-worktree-manager\\.(worktreeItem|gitFolderItem|folderItem|workspaceItem|fileItem)/",
          "group": "file-action@0"
        },
        {
          "command": "verition-worktree-manager.renameGitFolder",
          "when": "viewItem =~ /verition-worktree-manager\\.(gitFolderItem)/",
          "group": "file-action@1"
        },
        {
          "command": "verition-worktree-manager.addWorktree",
          "when": "viewItem =~ /verition-worktree-manager\\.(workspaceGitMainFolder|gitFolderItem)/",
          "group": "inline@2"
        },
        {
          "command": "verition-worktree-manager.switchToSelectWorkspace",
          "when": "viewItem =~ /verition-worktree-manager.worktreeItem/",
          "group": "inline@100"
        },
        {
          "command": "verition-worktree-manager.fetchWorktree",
          "when": "viewItem =~ /verition-worktree-manager.worktreeItem(.*)\\.fetch/ && config.verition-worktree-manager.treeView.showFetchInTreeItem",
          "group": "inline@9"
        },
        {
          "command": "verition-worktree-manager.pushWorktree",
          "when": "viewItem =~ /verition-worktree-manager.worktreeItem(.*)\\.ahead/",
          "group": "inline@10"
        },
        {
          "command": "verition-worktree-manager.pullWorktree",
          "when": "viewItem =~ /verition-worktree-manager.worktreeItem(.*)\\.behind/",
          "group": "inline@10"
        },
        {
          "command": "verition-worktree-manager.openExternalTerminal",
          "when": "viewItem =~ /verition-worktree-manager\\.(worktreeItem|gitFolderItem|folderItem)/ && config.verition-worktree-manager.treeView.showExternalTerminalInTreeItem",
          "group": "inline@12"
        },
        {
          "command": "verition-worktree-manager.openTerminal",
          "when": "viewItem =~ /verition-worktree-manager\\.(worktreeItem|gitFolderItem|folderItem)/ && config.verition-worktree-manager.treeView.showTerminalInTreeItem",
          "group": "inline@12"
        },
        {
          "command": "verition-worktree-manager.revealInSystemExplorer",
          "when": "viewItem =~ /verition-worktree-manager\\.(worktreeItem|gitFolderItem|folderItem|workspaceItem|fileItem)/ && config.verition-worktree-manager.treeView.showOpenExplorerInTreeItem",
          "group": "inline@12"
        },
        {
          "command": "verition-worktree-manager.checkoutBranch",
          "when": "viewItem =~ /verition-worktree-manager.worktreeItem.notBare/ && config.verition-worktree-manager.treeView.showCheckoutInWorktreeItem",
          "group": "inline@15"
        },
        {
          "command": "verition-worktree-manager.openRepository",
          "when": "viewItem =~ /verition-worktree-manager.worktreeItem.notBare/ && config.verition-worktree-manager.treeView.showOpenRepositoryInTreeItem",
          "group": "inline@16"
        },
        {
          "command": "verition-worktree-manager.lockWorktree",
          "when": "viewItem =~ /verition-worktree-manager.worktreeItem.notBare.unlock/ && config.verition-worktree-manager.treeView.showLockInWorktreeItem",
          "group": "inline@20"
        },
        {
          "command": "verition-worktree-manager.unlockWorktree",
          "when": "viewItem =~ /verition-worktree-manager.worktreeItem.notBare.lock/ && config.verition-worktree-manager.treeView.showLockInWorktreeItem",
          "group": "inline@20"
        },
        {
          "command": "verition-worktree-manager.checkoutBranch",
          "when": "viewItem =~ /verition-worktree-manager.worktreeItem.notBare/",
          "group": "git-action"
        },
        {
          "command": "verition-worktree-manager.openRepository",
          "when": "viewItem =~ /verition-worktree-manager.worktreeItem.notBare/",
          "group": "git-action"
        },
        {
          "command": "verition-worktree-manager.fetchRepo",
          "when": "viewItem =~ /verition-worktree-manager.gitFolderItem/ && config.verition-worktree-manager.treeView.showFetchInTreeItem",
          "group": "inline@1"
        },
        {
          "command": "verition-worktree-manager.renameGitFolder",
          "when": "viewItem =~ /verition-worktree-manager.gitFolderItem/",
          "group": "inline@1"
        },
        {
          "command": "verition-worktree-manager.viewHistory",
          "when": "viewItem =~ /verition-worktree-manager.(gitFolderItem|workspaceGitMainFolder)/",
          "group": "inline@1"
        },
        {
          "command": "verition-worktree-manager.gitFolderSetOpen",
          "when": "viewItem =~ /verition-worktree-manager.gitFolderItem(.*)\\.defaultClose/",
          "group": "inline@2"
        },
        {
          "command": "verition-worktree-manager.gitFolderSetClose",
          "when": "viewItem =~ /verition-worktree-manager.gitFolderItem(.*)\\.defaultOpen/",
          "group": "inline@2"
        },
        {
          "command": "verition-worktree-manager.bundleRepo",
          "when": "viewItem =~ /verition-worktree-manager.gitFolderItem/",
          "group": "inline@4"
        },
        {
          "command": "verition-worktree-manager.removeGitFolder",
          "when": "viewItem =~ /verition-worktree-manager.gitFolderItem/",
          "group": "inline@100"
        },
        {
          "command": "verition-worktree-manager.removeFavorite",
          "when": "view === verition-worktree-manager-favorite && viewItem =~ /verition-worktree-manager.(folderItem|workspaceItem|fileItem)/",
          "group": "inline@100"
        },
        {
          "command": "verition-worktree-manager.addToWorkspace",
          "when": "viewItem =~ /verition-worktree-manager.worktreeItem/ && !(viewItem =~ /verition-worktree-manager\\.worktreeItem(.*)\\.current/)",
          "group": "workspace@1"
        },
        {
          "command": "verition-worktree-manager.removeFromWorkspace",
          "when": "viewItem =~ /verition-worktree-manager.worktreeItem/ && viewItem =~ /verition-worktree-manager\\.worktreeItem(.*)\\.current/",
          "group": "workspace@1"
        },
        {
          "command": "verition-worktree-manager.copyFilePath",
          "when": "viewItem =~ /verition-worktree-manager.worktreeItem/",
          "group": "workspace@2"
        },
        {
          "command": "verition-worktree-manager.addToWorkspace",
          "when": "viewItem =~ /verition-worktree-manager.folderItem/",
          "group": "workspace@2"
        },
        {
          "command": "verition-worktree-manager.copyFilePath",
          "when": "viewItem =~ /verition-worktree-manager.(folderItem|fileItem)/",
          "group": "workspace@3"
        },
        {
          "command": "verition-worktree-manager.addToGitFolder",
          "when": "view === verition-worktree-manager-recent && viewItem =~ /verition-worktree-manager.folderItem/",
          "group": "workspace@1"
        },
        {
          "command": "verition-worktree-manager.addToFavorite",
          "when": "view === verition-worktree-manager-recent && viewItem =~ /verition-worktree-manager.(folderItem|workspaceItem|fileItem)/",
          "group": "workspace@1"
        },
        {
          "command": "verition-worktree-manager.switchToSelectWorkspace",
          "when": "viewItem =~ /verition-worktree-manager.(folderItem|workspaceItem|fileItem)/",
          "group": "inline@100"
        },
        {
          "group": "verition-worktree-action",
          "when": "viewItem =~ /verition-worktree-manager.worktreeItem/ && !(viewItem =~ /verition-worktree-manager\\.worktreeItem(.*)\\.main/)",
          "submenu": "verition-worktree-action"
        },
        {
          "command": "verition-worktree-manager.loadAllTreeData",
          "when": "viewItem =~ /verition-worktree-manager.loadMore/",
          "group": "inline@100"
        }
      ],
      "verition-worktree-action": [
        {
          "command": "verition-worktree-manager.repairWorktree",
          "when": "viewItem =~ /verition-worktree-manager.worktreeItem/",
          "group": "verition-worktree-action"
        },
        {
          "command": "verition-worktree-manager.moveWorktree",
          "when": "viewItem =~ /verition-worktree-manager.worktreeItem/",
          "group": "verition-worktree-action"
        },
        {
          "command": "verition-worktree-manager.removeWorktree",
          "when": "viewItem =~ /verition-worktree-manager.worktreeItem/",
          "group": "worktree-delete"
        },
        {
          "command": "verition-worktree-manager.lockWorktree",
          "when": "viewItem =~ /verition-worktree-manager\\.worktreeItem(.*)\\.unlock/",
          "group": "worktree-lock"
